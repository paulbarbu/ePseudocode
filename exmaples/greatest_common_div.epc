func greatestCommonDivisor(a, b)
    cat timp a != b executa
        daca a > b atunci
            a -= b
        altfel
            b -= a
        sf daca
    sf cat timp
    ret a
sf func

func gcd(a, b)
    daca b == 0 atunci
        ret a
    sf daca

    gcd(b, a % b)
sf func

func main()
    scrie(gcd(5, 25))
    scrie(greatestCommonDivisor(3, 5))
sf func
