//Fizz Buzz game in ePseudocode
func main()
    scrie("n=")
    n = int(citeste())

    daca fals == n atunci
        scrie("Invalid n!\n")
        ret 0
    sfdaca

    i = 1
    cattimp i <= n executa
        daca i % 3 == 0 si i % 5 == 0 atunci
            scrie("fizzbuzz\n")
        altfel daca i % 3 == 0 atunci
            scrie("fizz\n")
        altfel daca i % 5 == 0 atunci
            scrie("buzz\n")
        altfel
            scrie(i,"\n")
        sfdaca    sfdaca    sfdaca

        i = i + 1
    sfcattimp
sffunc
